    <owl:Axiom>
        <owl:annotatedSource>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.ease-crc.org/ont/SOMA.owl#SituationTransition"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/SOMA.owl/manifestsIn"/>
